    <application>
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>fr</supported-locale>
        </locale-config>

        <resource-bundle>
            <base-name>resources.messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>

</faces-config>